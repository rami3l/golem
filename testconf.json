{
  "name": "golem1",
  "title": "Golem",
  "tags": ["test", "python"],
  "testGroups": {
    "default": ["succ"]
  },
  "vars": {
    "$src": "py",
    "$bin": "pyc",
    "$stdin": "in",
    "$stdout": "out"
  },
  "run": ["cat $stdin | python ./golem.py $bin"],
  "binds": [{ "from": "src", "to": "/src", "options": "ro" }],
  "mapped_dir": "src"
}
